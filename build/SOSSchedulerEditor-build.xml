<?xml version="1.0" encoding="iso-8859-1"?>

<!-- ======================================================================= -->
<!-- Build com.sos.scheduler.editor                                          -->
<!-- ======================================================================= -->

<project name="editor" basedir=".">

	<import file="../AntHelper/TargetDef.xml"/>
	
    <property name="package.path" value="sos/scheduler" />
    <property name="package.pref" value="sos.scheduler" />
	<property name="jar.pref"     value="com.sos.scheduler"/>

    <property name="main.class.name" value="${package.pref}.${ant.project.name}.NoMainClass" />


	<!-- ======================================================================= -->
	<!--  all                                                                    -->
	<!-- ======================================================================= -->

	<target name="all"
	        depends="clean,compile,jar,doxygen.docu"
	        description="clean, compile, javadocs und danach jar erzeugen."
	/>
 

</project>